generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Analyze {
  id         String @id @default(uuid())
  identifier String @unique @db.Text()
  result     Json
}
